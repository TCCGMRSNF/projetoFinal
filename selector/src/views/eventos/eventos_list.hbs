<div>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/links">Coordenador</a></li>
            <li class="breadcrumb-item"><a href="/profile">Evento [00012]</a></li>
            <li class="breadcrumb-item active" aria-current="page">Notas</li>
        </ol>
    </nav>
</div>

<div class="table-wrapper-scroll-y my-custom-scrollbar">
    <table class="table table-striped table-sm table-hover mb-0">
        <thead>
            <tr>
                <th class="text-right">Id</th>
                <th>Descrição</th>
                <th class="text-right">Cand</th>
                <th class="text-right">Aprov</th>
                <th class="text-center">Início</th>
                <th class="text-center">Final</th>
                <th class="text-center">Última Edição</th>
            </tr>
        </thead>

        {{#each eventos}}
        <tr>
            <td class="text-right">{{id}}</td>
            <td>{{descricao}}</td>
            <td class="text-right">{{qtd_cand}}</td>
            <td class="text-right">{{qtd_aprov}}</td>
            <td class="text-center">{{fDataHora ts_ini '/'}}</td>
            <td class="text-center">{{fDataHora ts_fim '.'}}</td>
            <td class="text-center">{{fDataHora ts_upd '-'}}</td>
        </tr>

        {{/each}}

    </table>
</div>